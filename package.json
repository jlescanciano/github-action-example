  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlescanciano/github-action-example/issues"
  },
  "homepage": "https://github.com/jlescanciano/github-action-example#readme",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^2.1.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.3",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.5",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
